{
  "name": "mtrxyz",
  "version": "0.9.2",
  "type": "module",
  "main": "dist/mtrXYZ.cjs",  
  "module": "dist/mtrXYZ.mjs",  
  "exports": {
    ".": {
      "import": "./dist/mtrXYZ.mjs",
      "require": "./dist/mtrXYZ.cjs"
    },
    "./addon": {
      "import": "./dist/mtrXYZ_pathdata.mjs",
      "require": "./dist/mtrXYZ_pathdata.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/herrstrietzel/mtrXYZ.git"
  },
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^3.29.5",
    "rollup-plugin-concat": "^1.0.4"
  },
  "keywords": [
    "matrix",
    "3D-matrix",
    "2D-projection",
    "SVG"
  ],
  "author": "herrstrietzel",
  "license": "MIT",
  "description": "A helper for 3D and 2D matrix transformations",
  "dependencies": {
    "terser": "^5.39.0"
  }
}
